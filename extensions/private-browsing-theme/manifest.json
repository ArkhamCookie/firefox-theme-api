{
	"manifest_version": 2,
	"name": "Sneaky Theme",
	"version": "0.1.0",
	"description": "A theme that only shows up for private windows.",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["theme"],
	"browser_specific_settings": {
		"gecko": {
			"id": "private-window-theme@mozilla.org",
			"strict_min_version": "58.0"
		}
	}
}
